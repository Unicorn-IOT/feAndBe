{
	"name": "feandbe",
	"version": "1.0.0",
	"workspaces": [
		"BE",
		"FE"
	],
	"scripts": {
		"prepare": "husky install",
		"preupdate": "npx npm-check-updates -u",
		"update": "npm run update --workspaces",
		"postupdate": "npm install",
		"back:dev": "cd BE && npm run dev",
		"front:dev": "cd FE && npm run dev",
		"dev": "npm-run-all --parallel back:dev front:dev",
		"start":"cd FE && npm run start",
		"build": "cd FE && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Unicorn-IOT/feAndBe.git"
	},
